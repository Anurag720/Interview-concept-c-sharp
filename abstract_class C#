using System;

abstract class Animal
{
    public abstract void speak();
}

class Dog : Animal
{
    public override void speak()
    {
        Console.WriteLine("woof");
    }
}

class Cat : Animal
{
    public override void speak()
    {
        Console.WriteLine("meow");
    }
}

class Program
{
    static void Main()
    {
        Dog dog = new Dog();
        dog.speak();

        Cat cat = new Cat();
        cat.speak();
    }
}
